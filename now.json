{
  "name": "customerLeak",
  "version": 2,
  "builds": [{ "src": "src/index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
  "env": {
    "DB_USER": "@customerleak-db-user",
    "DB_PASSWORD": "@db-password",
    "DB_HOST": "@db-host",
    "DB_NAME": "@db-name"
  }
}
